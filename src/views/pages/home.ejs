<!DOCTYPE html>
<html lang="es">

<head>
  <%- include("../partials/head")%>
</head>

<body>
  <div class="container">
    <%- include('../partials/navbar') %>
      <p>
        <%- user?.email%>
      </p>
      <section class="movieContainer">
        <% movies?.forEach(function(movie) { %>
          <article class='movieCard'>
            <img src='<%- movie.poster %>' width='300' />
            <h3><%- movie.title %></h3>

          </article>
          <% }) %>
      </section>
  </div>

</body>

</html>